.push(am5xy.ValueAxis.new(root, {\n      extraTooltipPrecision: 1,\n      renderer: am5xy.AxisRendererY.new(root, {})\n    })); // Create X-Axis\n\n    let xAxis = chart.xAxes.push(am5xy.DateAxis.new(root, {\n      baseInterval: {\n        timeUnit: \"day\",\n        count: 1\n      },\n      renderer: am5xy.AxisRendererX.new(root, {})\n    }));\n    xAxis.get(\"dateFormats\")[\"day\"] = \"MM/dd\";\n    xAxis.get(\"periodChangeDateFormats\")[\"day\"] = \"MMMM\"; // Create series\n\n    function createSeries(name, field) {\n      var series = chart.series.push(am5xy.LineSeries.new(root, {\n        name: name,\n        xAxis: xAxis,\n        yAxis: yAxis,\n        valueYField: field,\n        valueXField: \"date\",\n        tooltip: am5.Tooltip.new(root, {})\n      }));\n      series.bullets.push(function () {\n        return am5.Bullet.new(root, {\n          sprite: am5.Circle.new(root, {\n            radius: 5,\n            fill: series.get(\"fill\")\n          })\n        });\n      });\n      series.strokes.template.set(\"strokeWidth\", 2);\n      series.get(\"tooltip\").label.set(\"text\", \"[bold]{name}[/]\\n{valueX.formatDate()}: {valueY}\");\n      series.data.setAll(data);\n    }\n\n    createSeries(\"Series\", \"value\"); // Add cursor\n\n    chart.set(\"cursor\", am5xy.XYCursor.new(root, {\n      behavior: \"zoomXY\",\n      xAxis: xAxis\n    }));\n    xAxis.set(\"tooltip\", am5.Tooltip.new(root, {\n      themeTags: [\"axis\"]\n    }));\n    yAxis.set(\"tooltip\", am5.Tooltip.new(root, {\n      themeTags: [\"axis\"]\n    }));\n  }\n\n};","map":{"version":3,"mappings":"AAQA,OAAO,KAAKA,GAAZ,MAAqB,qBAArB;AACA,OAAO,KAAKC,KAAZ,MAAuB,wBAAvB;AACA,OAAOC,kBAAP,MAA+B,qCAA/B,C,CACA;;AACA,eAAe;EACXC,IAAI,EAAE,eADK;;EAGXC,YAAY,GAAG;IACX,IAAI,CAACC,YAAY,CAACC,OAAb,CAAqB,iBAArB,CAAL,EAA8C;MAC1C,KAAKC,OAAL,CAAaC,IAAb,CAAkB;QAAEL,IAAI,EAAE;MAAR,CAAlB;IACJ;EACH,CAPU;;EASXM,OAAO,GAAG;IACN,MAAMC,KAAI,GAAI,KAAKC,MAAL,CAAYC,KAAZ,CAAkBC,IAAlB,CAAuBC,GAAvB,CAA4BC,CAAD,KACpC;MACGF,